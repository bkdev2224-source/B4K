# Site (for sitemap, Open Graph, canonical URLs)
# NEXT_PUBLIC_SITE_URL=https://b4korea.com

# Analytics (public - safe to commit placeholders)
NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX
NEXT_PUBLIC_CLARITY_ID=your_clarity_project_id
NEXT_PUBLIC_REQUIRE_ANALYTICS_CONSENT=false

# Rate limiting (optional - app works without; add for production)
# See docs/REDIS_SETUP.md for Upstash setup.
# UPSTASH_REDIS_REST_URL=
# UPSTASH_REDIS_REST_TOKEN=

# Auth (required for sign-in)
# NEXTAUTH_URL=
# NEXTAUTH_SECRET=  # min 32 chars in production: openssl rand -base64 32
# GOOGLE_CLIENT_ID=
# GOOGLE_CLIENT_SECRET=

# Database
# MONGODB_URI=

# Maps
# NEXT_PUBLIC_NAVER_MAP_CLIENT_ID=
